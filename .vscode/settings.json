{
  "autoHeader": {
    "format": {
      "startWith": "/**",
      "middleWith": "*",
      "endWith": "*/",
      "headerPrefix": "@"
    },
    "header": {
      "Author": "Amir W. Fathy",
      "AKA": "darkstar0x714",
      "Create Time": {
        "type": "createTime",
        "format": "YYYY-MM-DD HH:mm:ss"
      },
      "link": " ",
      "Description": " "
    }
  },
  "files.associations": {
    "array": "cpp",
    "deque": "cpp",
    "forward_list": "cpp",
    "list": "cpp",
    "string": "cpp",
    "unordered_map": "cpp",
    "unordered_set": "cpp",
    "vector": "cpp",
    "string_view": "cpp",
    "format": "cpp",
    "initializer_list": "cpp",
    "ranges": "cpp",
    "span": "cpp",
    "stacktrace": "cpp",
    "regex": "cpp",
    "valarray": "cpp"
  },
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  }
}

{
    "autoHeader": {
        "format": {
            "startWith": "/**",
            "middleWith": "*",
            "endWith": "*/",
            "headerPrefix": "@",
        },
        "header": {
            "Author": "Amir W. Fathy AKA darkstar0x714",
            "Create Time": {
                "type": "createTime",
                "format": "YYYY-MM-DD HH:mm:ss",
            },
            "problem link": {
                "type": "modifier",
                "value": "link_here",
            },
            "difficulty": {
                "type": "modifier",
                "value": "difficulty_here",
            },
            "tags": {
                "type": "modifier",
                "value": "tag_here",
            },
        }
    },
    "cSpell.words": [
        "Fathy"
    ]
}